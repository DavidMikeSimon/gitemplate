#!/usr/bin/env node

var cli = require('commander');
var Compake = require(__dirname + '/../dist/compake').Compake;

cli
  .option('-n, --name <component>', '', String)
  .parse(process.argv);


var compake = new Compake();
compake.set('name', cli.name);

console.log('input name:', cli.name);
console.log('setting name:', compake.get('name'));
